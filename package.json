{
    "name": "@milu/react-icons-converter",
    "version": "4.12.0",
    "description": "SVG icon converter to format required for use with react-icons package",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/patelmilanun/react-icons-converter.git"
    },
    "keywords": [
        "react-icons"
    ],
    "author": "Milan Patel",
    "license": "ISC",
    "homepage": "https://github.com/patelmilanun/react-icons-converter#readme",
    "files": [
        "lib/",
        "src/"
    ],
    "scripts": {
        "cleanup": "del-cli ./lib",
        "build": "npm run cleanup && tsc",
        "prepublishOnly": "npm run build",
        "prettier": "prettier --write **/*.{json,yml,js,ts,md}",
        "lint-staged": "lint-staged",
        "prepare": "is-ci || husky install",
        "eslint": "eslint .",
        "playground": "node ./playground/index.js"
    },
    "lint-staged": {
        "*.{json,yml,js,ts,md}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "camelcase": "8.0.0",
        "fast-xml-parser": "4.3.2",
        "react-icons-lib-only": "4.2.0"
    },
    "devDependencies": {
        "@types/node": "20.10.7",
        "@types/react": "18.2.47",
        "@typescript-eslint/eslint-plugin": "6.18.0",
        "@typescript-eslint/parser": "6.18.0",
        "del-cli": "5.1.0",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.2",
        "husky": "8.0.3",
        "is-ci": "3.0.1",
        "lint-staged": "15.2.0",
        "prettier": "3.1.1",
        "typescript": "5.3.3"
    }
}
